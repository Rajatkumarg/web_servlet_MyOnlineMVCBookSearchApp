<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>web_servlet_MyOnlineMVCBookSearchApp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
        <servlet-name>BookControllerServlet</servlet-name>
        <servlet-class>booksearch.mvc.BookControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookInsertServlet</servlet-name>
        <servlet-class>booksearch.mvc.BookInsertServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookDeleteServlet</servlet-name>
        <servlet-class>booksearch.mvc.BookDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BookControllerServlet</servlet-name>
        <url-pattern>/BookControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookInsertServlet</servlet-name>
        <url-pattern>/BookInsertServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookDeleteServlet</servlet-name>
        <url-pattern>/BookDeleteServlet</url-pattern>
    </servlet-mapping>
  
  
</web-app>